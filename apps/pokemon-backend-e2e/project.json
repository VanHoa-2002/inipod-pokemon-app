{
  "name": "pokemon-backend-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "implicitDependencies": ["pokemon-backend"],
  "targets": {
    "e2e": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
      "options": {
        "jestConfig": "apps/pokemon-backend-e2e/jest.config.ts",
        "passWithNoTests": true
      },
      "dependsOn": ["pokemon-backend:build", "pokemon-backend:serve"]
    }
  }
}
